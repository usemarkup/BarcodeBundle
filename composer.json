{
    "name": "markup/barcode-bundle",
    "description": "Barcode generation bundle wrapping Zend barcode component.",
    "keywords": ["barcode"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "douglas@usemarkup.com"
        },
        {
            "name": "Markup",
            "homepage": "http://www.usemarkup.com/"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.7|~3.0",
        "zendframework/zend-barcode": "~2.0|~3.0",
        "imagine/imagine": "~0.6.0",
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "twig/twig": "~1.28|~2.0"
    },
    "autoload": {
        "psr-0": { "Markup\\BarcodeBundle": "" }
    },
    "target-dir": "Markup/BarcodeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
